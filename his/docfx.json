{
  "build": {
    "content": [
      {
        "files": ["breadcrumb/toc.yml"]
      },
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/host-integration-server/breadcrumb/toc.json",
      "layout": "Conceptual",
      "titleSuffix": "Host Integration Server",
      "uhfHeaderId": "MSDocsHeader-His"
    },
    "fileMetadata": {
      "author": {
        "*.md": "haroldcampos",
        "**/*.md": "haroldcampos",
        "**/**/*.md": "haroldcampos",
        "troubleshooting/*.md": "tsnook"        
      },
      "manager": {
        "*.md": "mijacobs",
        "**/*.md": "mijacobs",
        "**/**/*.md": "mijacobs"
      },
      "ms.author": {
        "*.md": "hcampos",
        "**/*.md": "hcampos",
        "**/**/*.md": "hcampos",
        "troubleshooting/*.md": "tlsnook"
      },
      "titleSuffix": {"troubleshooting/*.md": "Host Integration Server"}
    },
    "template": [],
    "dest": "his",
    "markdownEngineName": "markdig"

  }
}
