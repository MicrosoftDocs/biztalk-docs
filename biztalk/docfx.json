{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "**/_themes/**",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/biztalk/breadcrumb/toc.json",
      "layout": "Conceptual",
      "titleSuffix": "BizTalk Server",
      "uhfHeaderId": "MSDocsHeader-BizTalk",
      "feedback_system": "Standard"
    },
    "fileMetadata": {
      "author": {
        "*.md": "haroldcampos",
        "**/*.md": "haroldcampos",
        "**/**/*.md": "haroldcampos",
        "troubleshooting/*.md": "haroldcampos"
      },
      "manager": {
        "*.md": "mijacobs",
        "**/*.md": "mijacobs",
        "**/**/*.md": "mijacobs"
      },
      "ms.author": {
        "*.md": "hcampos",
        "**/*.md": "hcampos",
        "**/**/*.md": "hcampos",
        "troubleshooting/*.md": "hcampos"        
      },
      "titleSuffix": {"troubleshooting/*.md": "BizTalk Server"}
    },
    "template": [],
    "dest": "biztalk",
    "markdownEngineName": "markdig"
  }
}
